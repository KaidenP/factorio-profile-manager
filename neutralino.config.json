{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "github.KaidenP.FPM",
  "version": "2.0.0",
  "defaultMode": "window",
  "//defaultMode": "chrome",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.*",
    "window.*",
    "os.*",
    "computer.*",
    "filesystem.*"
  ],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "Factorio Profile Manager",
      "width": 960,
      "height": 540,
      "minWidth": 960,
      "minHeight": 540,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/appIcon.png",
      "enableInspector": false,
      "borderless": true,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": true,
      "webviewArgs": "--msEdgeDevToolsWdpRemoteDebugging --remote-debugging-port=9222"
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--remote-debugging-port=9222",
      "nativeBlockList": []
    }
  },
  "cli": {
    "binaryName": "factorio-profile-manager",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.4.0",
    "clientVersion": "6.4.0"
  }
}